	ORG 0
BEG,	LDA PTA I
	STA TMA
	SPA
	BSA CM	;-ve=needs to be +ve
TSN,	LDA PTB I
	STA TMB
	SPA
	BSA CN	;-ve= needs to be +ve
MUL,	LDA TMB
	CMA
	INC
	STA CTR
	SNA
	BUN CON
LOP,	LDA PTS I
	ADD TMA
	STA PTS I
	ISZ CTR
	BUN LOP
	LDA CHK
	SZA
	BSA NEG
	BUN CON
CM,	HEX 0	;complements A
	SNA
	BUN CON
	CMA
	INC
	STA TMA
	BSA INV
	BUN CM I
CN,	HEX 0	;complements B
	SNA
	BUN CON
	CMA
	INC
	STA TMB
	BSA INV	;for -ve
	BUN CN I
CON,	ISZ I
	BUN NXT
	BUN STP
NXT,	ISZ PTA 
	ISZ PTB 
	ISZ PTS 
	BUN BEG
INV,	HEX 0
	LDA CHK
	CMA
	STA CHK
	BUN INV I
NEG,	HEX 0
	LDA PTS I
	CMA
	INC
	STA PTS I
	CLA
	STA CHK
	BUN NEG I
STP,	HLT

CTR,	DEC 0
CHK,	DEC 0
TMA,	DEC 0
TMB,	DEC 0
I,	DEC -10
PTA,	DEC 70
PTB,	DEC 80
PTS,	DEC 90

	ORG 70
ArA,	DEC 1
	DEC -2
	DEC 3
	DEC -4
	DEC 0
	DEC 6
	DEC 0
	DEC 8
	DEC 0
	DEC 10
ArB,	DEC 1
	DEC 2
	DEC -3
	DEC -4
	DEC 5
	DEC 0
	DEC 0
	DEC 8
	DEC 9
	DEC 10
ArS,	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	DEC 0
	END